            "rawSql": "SELECT\n  DATE_FORMAT(Fecha, \n    CASE \n      WHEN '$intervalo' = 'day' THEN '%Y-%m-%d'\n      WHEN '$intervalo' = 'week' THEN CONCAT(YEAR(Fecha), '-W', WEEK(Fecha))\n      WHEN '$intervalo' = 'month' THEN '%Y-%m'\n    END\n  ) AS time,\n  Estado,\n  COUNT(*) AS cantidad\nFROM Pacientes\nWHERE $__timeFilter(Fecha)\n  AND Estado IN ('proceso', 'Alta')\nGROUP BY time, Estado\nORDER BY time ASC;\n",
